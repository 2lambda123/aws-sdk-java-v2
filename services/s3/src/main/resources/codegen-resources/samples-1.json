{
  "samples": {
    "BasicBucketAndObjectOperations": {
      "documentation": "This example demonstrates how to get started with S3 buckets and objects by performing basic CRUD operations on S3 buckets and objects\n",
      "instructions": [
        {
          "name": "CreateBucket",
          "type": "ServiceOperation",
          "documentation": "Create an S3 bucket",
          "input": [
            {
              "name": "Bucket",
              "type": "string",
              "value": "foo"
            }
          ],
          "outputConsumer": {
            "outputVariable": "createBucketResponse",
            "instructions": [
              {
                "name": "print",
                "type": "SdkOperation",
                "input": [
                  {
                    "type": "string",
                    "value": "Response: "
                  },
                  {
                    "type": "reference",
                    "value": "createBucketResponse"
                  }
                ]
              }
            ]
          }
        },
        {
          "name": "BucketExists",
          "type": "ServiceWaiter",
          "documentation": "Wait until the bucket has been created",
          "input": [
            {
              "name": "Bucket",
              "type": "string",
              "value": "foo"
            }
          ]
        },
        {
          "name": "PutObject",
          "type": "ServiceOperation",
          "documentation": "Upload a local file \"source.txt\" to the bucket",
          "input": [
            {
              "name": "Bucket",
              "type": "string",
              "value": "foo"
            },
            {
              "name": "Key",
              "type": "string",
              "value": "bar"
            }
          ],
          "streamingInput": {
            "type": "file",
            "value": "source.txt"
          }
        },
        {
          "name": "GetObject",
          "type": "ServiceOperation",
          "documentation": "Download the S3 object to a local file",
          "input": [
            {
              "name": "Bucket",
              "type": "string",
              "value": "foo"
            },
            {
              "name": "Key",
              "type": "string",
              "value": "bar"
            }
          ],
          "streamingOutput": {
            "type": "file",
            "value": "destination.txt"
          }
        },
        {
          "name": "ListObjectsV2",
          "type": "ServiceOperation",
          "documentation": "List all objects in the bucket",
          "usePaginator": true,
          "input": [
            {
              "name": "Bucket",
              "type": "string",
              "value": "foo"
            }
          ]
        },
        {
          "name": "DeleteObject",
          "type": "ServiceOperation",
          "documentation": "Delete the object",
          "input": [
            {
              "name": "Bucket",
              "type": "string",
              "value": "foo"
            },
            {
              "name": "Key",
              "type": "string",
              "value": "bar"
            }
          ]
        },
        {
          "name": "DeleteBucket",
          "type": "ServiceOperation",
          "documentation": "Delete the bucket",
          "input": [
            {
              "name": "Bucket",
              "type": "string",
              "value": "foo"
            }
          ]
        },
        {
          "name": "BucketNotExists",
          "type": "ServiceWaiter",
          "documentation": "Wait until the bucket has been deleted",
          "input": [
            {
              "name": "Bucket",
              "type": "string",
              "value": "foo"
            }
          ]
        }
      ]
    }
  }
}
